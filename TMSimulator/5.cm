/*This is a stripped down version of the invSqrt*/
int Q_rsqrt( int number )
{
    int i;
    int x2; int y;
    int threehalfs;
    threehalfs = 3/2;


    x2 = number * 1/2;
    y  = number;
    i  = 21 - ( i + 1 );
    y  = y * ( threehalfs - ( x2 * y * y ) );

    return y;
}

void main(void)
{
    int a;
    a = Q_rsqrt(200);
    return a;
}
